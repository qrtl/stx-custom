<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="report_yamato_csv" model="ir.actions.report">
        <field name="name">Yamato CSV</field>
        <field name="model">stock.picking</field>
        <field name="report_type">csv</field>
        <field name="encoding">cp932</field>
        <field name="report_name">stock_picking_yamato_csv.report_yamato_csv</field>
        <field name="report_file">stock_picking_yamato_csv.report_yamato_csv</field>
        <field name="print_report_name">'Yamato_%s' % (time.strftime('%Y%m%d'))</field>
        <field name="binding_model_id" ref="model_stock_picking" />
    </record>
</odoo>
